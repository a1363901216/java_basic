import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class Test103reg {


    public static void main(String[] args) {

        String s = "service12@xsoftlab.net";
        Pattern p = Pattern.compile("([a-zA-Z_]+)([a-zA-Z_0-9]*)@([a-zA-Z0-9]+)\\.([a-zA-X]+)");
        Matcher m = p.matcher(s);
        System.out.println(m.matches());
        System.out.println(s.indexOf("vice"));



    }
}
